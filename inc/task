TASKPATH="${INCPATH}/targets/${TASK}"

if [ ! -f "${TASKPATH}" ]
then
  echo "****************************************************************************"
  echo "** Unable to find target configuration for ${TASK}"
  echo "****************************************************************************"

  exit 1
fi

ENTRYPOINT=${ENTRYPOINT:-""}
ENTRYPOINTARGS=${ENTRYPOINTARGS:-""} 
echo "# Found target configuration for ${TASK}"
. "${TASKPATH}"
